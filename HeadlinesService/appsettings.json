{
  "spring": {
    "application": {
      "name": "headlines-service"
    },
    "cloud": {
      "config": {
        "validate_certificates": false,
        "failFast": false,
        "discovery": {
          "enabled": true
        },
        "health": {
          "enabled": true
        }
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": {
        "defaultZone": "http://localhost:8761/eureka/"
      },
      "shouldFetchRegistry": false,
      "validateCertificates": false
    },
    "instance": {
      "hostname": "localhost",
      "port": "5100"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
