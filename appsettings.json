{
  "spring": {
    "application": {
      "name": "demo"
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validate_certificates": false,
        "failFast": true,
        "discovery": {
          "enabled": false
        },
        "health": {
          "enabled": true
        }
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": true,
      "ValidateCertificates": false
    },
    "instance": {
      "hostname": "localhost"
    }
  },
  "hystrix": {
    "stream": {
      "validate_certificates": false
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Steeltoe": "Debug"
    }
  },
  "AllowedHosts": "*"
}
