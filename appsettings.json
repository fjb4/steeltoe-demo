{
  "spring": {
    "application": {
      "name": "demo"
    },
    "cloud": {
      "config": {
        "validate_certificates": false,
        "failFast": false,
        "discovery": {
          "enabled": true
        },
        "health": {
          "enabled": true
        }
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": {
        "defaultZone": "http://localhost:8761/eureka/"
      },
      "shouldFetchRegistry": true,
      "ValidateCertificates": false
    },
    "instance": {
      "hostname": "localhost"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Steeltoe": "Debug"
    }
  },
  "AllowedHosts": "*"
}
